@model IEnumerable<CondominioInteligente.Models.SolicitacaoAreaComum>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataFim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.aprovado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataFim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.aprovado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodSolictacao">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodSolictacao">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodSolictacao">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
