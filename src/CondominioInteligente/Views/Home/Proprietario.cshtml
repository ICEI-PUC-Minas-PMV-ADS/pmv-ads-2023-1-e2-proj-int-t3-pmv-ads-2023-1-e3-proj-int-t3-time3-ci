<div id="exibicao"></div>
<script type="text/javascript">

    function carregarCadastroSolicitacaoAreaComum() {

        $.get('@Url.Action("Listagem", "SolicitacaoAreaComums")', {}, function (data) {

            $("#exibicao").empty();
            $("#exibicao").html(data);

        });
    }
    function carregarNotificacoes() {

        $.get('@Url.Action("Listagem", "SolicitacaoAreaComums")', {}, function (data) {

            $("#exibicao").empty();
            $("#exibicao").html(data);

        });
    }
    function CarregarAtasReunicao() {

        $.get('@Url.Action("Listagem", "SolicitacaoAreaComums")', {}, function (data) {

            $("#exibicao").empty();
            $("#exibicao").html(data);

        });
    }
</script>
